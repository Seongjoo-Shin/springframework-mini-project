<?xml version="1.0" encoding="UTF-8" ?> <!-- 이 태그는 항상 첫번째 줄에 작성한다, xml 선언부 -->

<!-- 문서 유형(타입) 선언, 루트 엘리먼트에 대한 설명 -->
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd"> <!-- 루트태그에서 작성될 태그들이 여기에 작성되어 있음을 알려줌 -->

<configuration> <!-- 루트 태그, 루트 엘리먼트, 루트 태그는 항상 1개만 있어야함 -->
	<typeAliases>
		<typeAlias type="com.mycompany.webapp.dto.BuildingDto" alias="building" />
		<typeAlias type="com.mycompany.webapp.dto.BuildingFileDto" alias="buildingfile" />
		<typeAlias type="com.mycompany.webapp.dto.CommentDto" alias="comment" />
		<typeAlias type="com.mycompany.webapp.dto.EquipmentDto" alias="equipment" />
		<typeAlias type="com.mycompany.webapp.dto.FreeBoardDto" alias="freeboard" />
		<typeAlias type="com.mycompany.webapp.dto.KeyWordDto" alias="keyword" />
		<typeAlias type="com.mycompany.webapp.dto.LikeListDto" alias="likelist" />
		<typeAlias type="com.mycompany.webapp.dto.MarketBoardDto" alias="marketboard" />
		<typeAlias type="com.mycompany.webapp.dto.MarketFileDto" alias="marketfile" />
		<typeAlias type="com.mycompany.webapp.dto.MessageDto" alias="message" />
		<typeAlias type="com.mycompany.webapp.dto.NoticeBoardDto" alias="noticeboard" />
		<typeAlias type="com.mycompany.webapp.dto.NoticeFileDto" alias="noticefile" />
		<typeAlias type="com.mycompany.webapp.dto.PagerDto" alias="pager" />
		<typeAlias type="com.mycompany.webapp.dto.UserDto" alias="user" />
		<typeAlias type="com.mycompany.webapp.dto.WishListDto" alias="wishlist" />
		<typeAlias type="com.mycompany.webapp.dto.MarketPagerDto" alias="marketpager" />
		<typeAlias type="com.mycompany.webapp.dto.MarketAlignDto" alias="marketalign" />
   </typeAliases>
</configuration>